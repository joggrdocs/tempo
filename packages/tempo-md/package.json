{
  "name": "@joggr/tempo-md",
  "description": "A package for handling markdown-related functionality in the Tempo ecosystem.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joggrdocs/tempo.git",
    "directory": "packages/tempo-md"
  },
  "author": "Joggr Team <engineering@joggr.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joggrdocs/tempo/issues"
  },
  "homepage": "https://github.com/joggrdocs/tempo/packages/tempo-md#readme",
  "type": "module",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "rslib build --watch",
    "build": "rslib build",
    "test": "vitest --coverage",
    "analyze:types": "tsc --noEmit"
  },
  "devDependencies": {
    "@rslib/core": "^0.5.5",
    "@types/node": "^22.13.13",
    "@vitest/coverage-istanbul": "^3.0.9",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  },
  "dependencies": {
    "marked": "^12.0.0"
  }
}
